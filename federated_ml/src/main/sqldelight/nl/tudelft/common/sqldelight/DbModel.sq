import kotlin.collections.List;

CREATE TABLE models (
    name TEXT NOT NULL,
    type TEXT NOT NULL,
    parameters TEXT NOT NULL,
    public_key BLOB NOT NULL,
    PRIMARY KEY (public_key)
);

addModel:
REPLACE INTO models (name, type, parameters, public_key)
VALUES(?, ?, ?, ?);

getModel:
SELECT * FROM models WHERE public_key = ?;

getAll:
SELECT * FROM models;

deleteModel:
DELETE FROM models WHERE public_key = ?;