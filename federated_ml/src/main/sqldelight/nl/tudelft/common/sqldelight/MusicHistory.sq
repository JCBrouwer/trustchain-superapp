import kotlin.collections.List;

CREATE TABLE musichistory (
    times_played INT NOT NULL,
    owned BOOL NOT NULL,

    -- hash to the block the song info is stored in
    hash TEXT NOT NULL,
    PRIMARY KEY (hash)
);

addModel:
REPLACE INTO models (times_played owned, hash)
VALUES(?, ?, ?);

getModel:
SELECT * FROM models WHERE public_key = ?;

getAll:
SELECT * FROM models;

deleteModel:
DELETE FROM models WHERE public_key = ?;